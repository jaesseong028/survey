@model UBSurvey.Models.V_ProgressInfo;

@{
    ViewData["Title"] = "Progress";
    Layout = "~/Views/Shared/Survey/_Layout.cshtml";
    
}

<link rel="stylesheet" type="text/css" media="screen" href="~/css/Example/main2.css" />

<environment include="Development">
    <script src="~/js/Example/view2.js"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>        
    <script src="~/js/axios.min.js"></script>
</environment>
<environment include="Production">    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>        
    <script src="~/js/Example/view2.js"></script>
    <script src="~/js/axios.min.js"></script>
</environment>

<script>

    var surveyInfo = {
        info : '@Model._info',
        strSurvey : '@Html.Raw(Model.Survey)',
        sendReuslt : function(result){

            axios.post('/Survey/Complete', { _info : this.info, _surveyResult : result})
            .then(function (response) {
                console.log(response);
            })
            .catch(function (error) {
                console.log(error);
            });
        }
    };
    
</script>


<div id="ubSurvey"></div>

